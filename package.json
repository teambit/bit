    "chalk": "^2.4.1",
    "chokidar": "^3.0.2",
    "cli-spinners": "^1.0.0",
    "commander": "^2.12.2",
    "comment-json": "^1.1.3",
    "debug": "^4.1.1",
    "decamelize": "^1.2.0",
    "detect-indent": "^5.0.0",
    "detect-newline": "^3.0.0",
    "doctrine": "^2.0.2",
    "execa": "^2.0.3",
    "find-up": "^2.1.0",
    "firstline": "^2.0.2",
    "fs-extra": "^6.0.0",
    "glob": "^7.1.1",
    "graphlib": "^2.1.5",
    "graphviz": "0.0.9",
    "group-array": "^0.3.4",
    "ignore": "^3.3.7",
    "inquirer": "^6.5.0",
    "inquirer-fuzzy-path": "^2.0.3",
    "is-array-buffer-x": "^1.0.13",
    "is-glob": "^4.0.0",
    "isbinaryfile": "^4.0.2",
    "jfs": "^0.2.6",
    "lodash.assignwith": "^4.2.0",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.partition": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.toarray": "^4.4.0",
    "lodash.unionby": "^4.8.0",
    "lodash.uniqby": "^4.7.0",
    "loud-rejection": "^1.6.0",
    "make-fetch-happen": "^4.0.1",
    "memfs": "2.15.5",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "mockery": "^2.0.0",
    "needle": "^2.0.0",
    "normalize-path": "^2.1.1",
    "object-diff": "^0.0.4",
    "object-hash": "^1.3.0",
    "opn": "^5.3.0",
    "ora": "^1.1.0",
    "p-event": "^4.1.0",
    "p-map-series": "^1.0.0",
    "p-series": "^2.1.0",
    "package-json-validator": "^0.6.3",
    "pad-right": "^0.2.2",
    "parse-gitignore": "^1.0.1",
    "porter-stemmer": "^0.9.1",
    "prompt": "^1.0.0",
    "ramda": "^0.24.1",
    "ramda-adjunct": "^2.9.0",
    "react-docgen": "^2.21.0",
    "readable-stream": "^3.4.0",
    "regenerator-runtime": "^0.11.0",
    "requestify": "^0.2.5",
    "semver": "^5.4.1",
    "serialize-error": "^5.0.0",
    "ssh2": "^0.5.4",
    "string-format": "^0.5.0",
    "stringify-package": "^1.0.0",
    "symlink-or-copy": "^1.1.8",
    "table": "^5.2.3",
    "tar-stream": "^2.0.1",
    "tty-table": "^2.6.9",
    "uid-number": "0.0.6",
    "unionfs": "^3.0.2",
    "uniqid": "^5.0.3",
    "user-home": "^2.0.0",
    "uuid": "^3.0.1",
    "v8-compile-cache": "^2.0.2",
    "validate-npm-package-name": "^3.0.0",
    "vinyl": "^2.2.0",
    "vinyl-file": "^3.0.0",
    "winston": "^3.2.1",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.4.4",
