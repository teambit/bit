{
  "name": "bit-bin",
  "version": "14.2.2",
  "license": "Apache-2.0",
  "main": "./dist/api.js",
  "preferGlobal": true,
  "scripts": {
    "dev-link": "node ./scripts/establish-dev-link.js $1",
    "precommit": "lint-staged",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow-coverage": "flow-coverage-report -i \"./src/**/*.js\"",
    "lint": "eslint \"{src,e2e}/**/*.js\" && flow check",
    "lint:table": "eslint \"{src,e2e}/**/*.js\" --format table",
    "lint:html": "eslint \"{src,e2e}/**/*.js\" --format html -o eslint-report.html",
    "lint-circle": "eslint \"{src,e2e}/**/*.js\" --ignore-path .eslintignore-circle --format junit -o junit/eslint-results.xml",
    "lint-circle-debug": "eslint \"{src,e2e}/**/*.js\" --ignore-path .eslintignore-circle",
    "format": "prettier-eslint \"{src,e2e}/**/*.js\" --write",
    "test": "mocha --require @babel/register './src/**/*.spec.js'",
    "test:appveyor": "mocha --require @babel/register --reporter mocha-appveyor-reporter ./src/**/*.spec.js",
    "test-circle": "mocha --require @babel/register --reporter mocha-circleci-reporter './src/**/*.spec.js'",
    "mocha-circleci": "mocha --require @babel/register --reporter mocha-circleci-reporter",
    "e2e-test": "mocha --require @babel/register ./e2e/**/*.e2e*.js",
    "e2e-test:appveyor1": "mocha --require @babel/register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.1.js 2> output.err",
    "e2e-test:appveyor2": "mocha --require @babel/register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.2.js 2> output.err",
    "e2e-test:appveyor3": "mocha --require @babel/register --reporter mocha-appveyor-reporter ./e2e/**/*.e2e.3.js 2> output.err",
    "e2e-test:appveyor-debug-example": "set npm_config_debug=true && mocha --require @babel/register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.2.js --reporter-options appveyorBatchSize=1",
    "e2e-test-circle": "mocha --require @babel/register --reporter mocha-circleci-reporter ./e2e/**/*.e2e*.js",
    "performance-test": "mocha --require @babel/register ./e2e/performance/*.performance*.js",
    "performance-test-circle": "mocha --require @babel/register --reporter mocha-circleci-reporter ./e2e/performance/*.performance*.js",
    "clean": "rm -rf dist",
    "build": "babel src -d dist --verbose",
    "build-centos-image": "docker build ./scripts/linux/centos -t centos-rpm",
    "build-debian-image": "docker build ./scripts/linux/debian -t debian-deb",
    "pkg": "pkg bin/bit.js --targets node10 --out-path releases/ --options --no-warnings --config package.json",
    "pkg:linux": "pkg bin/bit.js --targets node10-linux-x64 --out-path releases/linux --options --no-warnings --config package.json",
    "pkg:mac": "pkg bin/bit.js --targets node10-macos-x64 --out-path releases/mac --options --no-warnings --config package.json",
    "pkg:windows": "pkg bin/bit.js --targets node10-win-x64 --out-path releases/windows --options --no-warnings --config package.json",
    "pkg:all": "pkg bin/bit.js --targets node10-macos-x64,node10-win-x64,node10-linux-x64 --out-path releases/ --options --no-warnings --config package.json",
    "watch": "babel --watch bin src -d dist --source-maps --verbose",
    "build-debug": "babel src -d dist --source-maps",
    "prepublishOnly": "npm run clean && npm run build",
    "pre-release": "npm run pkg:all && gh-release -a releases/bit-bin-linux,releases/bit-bin-win.exe,releases/bit-bin-macos --prerelease",
    "release": "npm run pkg:all && gh-release -a releases/bit-bin-linux,releases/bit-bin-win.exe,releases/bit-bin-macos"
  },
  "pkg": {
    "assets": [
      "dist/analytics/analytics-sender.js",
      "dist/specs-runner/worker.js"
    ]
  },
  "bin": {
    "bit": "bin/bit.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teambit/bit"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@teambit/gitconfig": "^2.0.6",
    "ajv": "^6.5.3",
    "array-difference": "^0.0.1",
    "async-exit-hook": "^2.0.1",
    "babel-runtime": "^6.23.0",
    "bit-javascript": "^2.1.0",
    "bluebird": "^3.5.3",
    "chalk": "^2.4.1",
    "chokidar": "^3.0.2",
    "cli-spinners": "^1.0.0",
    "commander": "^2.12.2",
    "comment-json": "^1.1.3",
    "debug": "^4.1.1",
    "decamelize": "^1.2.0",
    "deserialize-error": "0.0.3",
    "detect-indent": "^5.0.0",
    "detect-newline": "^3.0.0",
    "doctrine": "^2.0.2",
    "execa": "^2.0.3",
    "find-up": "^2.1.0",
    "firstline": "^2.0.2",
    "fs-extra": "^6.0.0",
    "glob": "^7.1.1",
    "graphlib": "^2.1.5",
    "group-array": "^0.3.4",
    "ignore": "^3.3.7",
    "is-array-buffer-x": "^1.0.13",
    "is-glob": "^4.0.0",
    "isbinaryfile": "^4.0.2",
    "jfs": "^0.2.6",
    "lodash.assignwith": "^4.2.0",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.partition": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.toarray": "^4.4.0",
    "lodash.unionby": "^4.8.0",
    "lodash.uniqby": "^4.7.0",
    "loud-rejection": "^1.6.0",
    "make-fetch-happen": "^4.0.1",
    "memfs": "^2.15.4",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "mockery": "^2.0.0",
    "needle": "^2.0.0",
    "normalize-path": "^2.1.1",
    "object-diff": "^0.0.4",
    "object-hash": "^1.3.0",
    "opn": "^5.3.0",
    "ora": "^1.1.0",
    "p-event": "^4.1.0",
    "p-map-series": "^1.0.0",
    "pad-right": "^0.2.2",
    "parse-gitignore": "^1.0.1",
    "porter-stemmer": "^0.9.1",
    "prompt": "^1.0.0",
    "ramda": "^0.24.1",
    "ramda-adjunct": "^2.9.0",
    "react-docgen": "^2.21.0",
    "readable-stream": "^3.4.0",
    "regenerator-runtime": "^0.11.0",
    "requestify": "^0.2.5",
    "semver": "^5.4.1",
    "serialize-error": "^2.1.0",
    "ssh2": "^0.5.4",
    "string-format": "^0.5.0",
    "stringify-package": "^1.0.0",
    "symlink-or-copy": "^1.1.8",
    "table": "^5.2.3",
    "tar-stream": "^2.0.1",
    "tty-table": "^2.6.9",
    "typescript": "3.2.4",
    "uid-number": "0.0.6",
    "unionfs": "^3.0.2",
    "uniqid": "^5.0.3",
    "user-home": "^2.0.0",
    "uuid": "^3.0.1",
    "v8-compile-cache": "^2.0.2",
    "validate-npm-package-name": "^3.0.0",
    "vinyl": "^2.1.0",
    "vinyl-file": "^3.0.0",
    "winston": "^2.4.0",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "chai-arrays": "^1.0.0",
    "chai-fs": "^1.0.0",
    "chai-string": "^1.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.75.0",
    "flow-coverage-report": "^0.6.1",
    "gh-release": "^3.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "mocha": "^5.1.1",
    "mocha-appveyor-reporter": "^0.4.2",
    "mocha-circleci-reporter": "0.0.3",
    "mocha-junit-reporter": "^1.13.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "sinon": "^5.0.7",
    "tar": "^4.4.8",
    "verdaccio": "^4.1.0"
  },
  "files": [
    "/bin",
    "/components",
    "/dist"
  ],
  "lint-staged": {
    "*.{js,css}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
