{
  "name": "bit-javascript",
  "version": "1.0.6-dev.5",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src && flow check || true",
    "precommit": "lint-staged",
    "format": "prettier-eslint src/**/*.js --write",
    "test": "mocha --require babel-core/register './src/**/*.spec.js'",
    "test-circle": "mocha --require babel-core/register --reporter mocha-circleci-reporter './src/**/*.spec.js'",
    "test-appveyor": "mocha --require babel-core/register --reporter mocha-appveyor-reporter \"./src/**/*.spec.js\"",
    "clean": "rm -rf dist",
    "build": "babel src -d dist  --source-maps",
    "watch": "babel --watch src -d dist --source-maps",
    "prepublishOnly": "npm run clean && npm run build",
    "build-debug": "babel src -d dist --source-maps"
  },
  "bin": {
    "bitjs": "bin/bitjs.js"
  },
  "homepage": "https://github.com/teambit/bit-javascript",
  "repository": {
    "type": "git",
    "url": "https://github.com/teambit/bit-javascript"
  },
  "keywords": [
    "bit",
    "node",
    "components",
    "bit-node",
    "bit-js",
    "bit-component",
    "bit-components"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "babel-runtime": "^6.26.0",
    "camelcase": "^4.0.0",
    "caporal": "^0.10.0",
    "chalk": "^1.1.3",
    "commander": "^2.14.1",
    "debug": "^3.1.0",
    "detective-amd": "^2.4.0",
    "detective-stylus": "^1.0.0",
    "enhanced-resolve": "^4.0.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.1",
    "gonzales-pe": "^4.2.3",
    "ini-builder": "^1.1.1",
    "is-relative-path": "^2.0.0",
    "lodash.partition": "^4.6.0",
    "lodash.set": "^4.3.2",
    "module-definition": "^2.2.4",
    "module-lookup-amd": "^5.0.1",
    "node-source-walk": "^4.0.0",
    "object-assign": "^4.1.1",
    "ora": "^1.2.0",
    "parents": "^1.0.1",
    "ramda": "^0.22.1",
    "regenerator-runtime": "^0.10.5",
    "resolve": "^1.5.0",
    "resolve-dependency-path": "^1.0.2",
    "sass-lookup": "^1.1.0",
    "stylable": "5.2.2",
    "stylus-lookup": "^1.0.2",
    "typescript": "2.9.1",
    "typescript-eslint-parser": "16.0.0",
    "user-home": "^2.0.0",
    "vue-template-compiler": "^2.5.13"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.72.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "mocha": "^5.1.1",
    "mocha-appveyor-reporter": "^0.4.0",
    "mocha-circleci-reporter": "0.0.3",
    "mocha-junit-reporter": "^1.13.0",
    "mock-fs": "^4.5.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rewire": "^4.0.1",
    "sinon": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
