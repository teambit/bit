{
  "name": "bit-javascript",
  "version": "0.10.9-dev.1",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src && flow check || true",
    "test": "jest",
    "test-watch": "jest --watch",
    "clean": "rm -rf dist",
    "build": "babel src -d dist",
    "watch": "babel --watch src -d dist --source-maps",
    "prepublishOnly": "npm run clean && npm run build",
    "build-debug": "babel src -d dist --source-maps"
  },
  "bin": {
    "bitjs": "bin/bitjs.js"
  },
  "homepage": "https://github.com/teambit/bit-javascript",
  "repository": {
    "type": "git",
    "url": "https://github.com/teambit/bit-javascript"
  },
  "keywords": [
    "bit",
    "node",
    "components",
    "bit-node",
    "bit-js",
    "bit-component",
    "bit-components"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "camelcase": "^4.0.0",
    "caporal": "^0.5.0",
    "chalk": "^1.1.3",
    "dependency-tree": "teambit/node-dependency-tree",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.1",
    "lodash.partition": "^4.6.0",
    "ora": "^1.2.0",
    "parents": "^1.0.1",
    "ramda": "^0.22.1",
    "regenerator-runtime": "^0.10.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "flow-bin": "^0.43.0",
    "jest-cli": "^20.0.3",
    "mock-fs": "^4.2.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
