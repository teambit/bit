{
  "name": "bit-bin",
  "version": "13.0.5-dev.10",
  "license": "Apache-2.0",
  "main": "./dist/api.js",
  "preferGlobal": true,
  "scripts": {
    "dev-link": "node ./scripts/establish-dev-link.js $1",
    "precommit": "lint-staged",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow-coverage": "flow-coverage-report -i \"./src/**/*.js\"",
    "lint": "eslint \"{src,e2e}/**/*.js\" && flow check",
    "lint:table": "eslint \"{src,e2e}/**/*.js\" --format table",
    "lint:html": "eslint \"{src,e2e}/**/*.js\" --format html -o eslint-report.html",
    "lint-circle": "eslint \"{src,e2e}/**/*.js\" --ignore-path .eslintignore-circle --format junit -o junit/eslint-results.xml",
    "lint-circle-debug": "eslint \"{src,e2e}/**/*.js\" --ignore-path .eslintignore-circle",
    "format": "prettier-eslint \"{src,e2e}/**/*.js\" --write",
    "test": "mocha --require babel-core/register './src/**/*.spec.js'",
    "test:appveyor": "mocha --require babel-core/register --reporter mocha-appveyor-reporter ./src/**/*.spec.js",
    "test-circle": "mocha --require babel-core/register --reporter mocha-circleci-reporter './src/**/*.spec.js'",
    "mocha-circleci": "mocha --require babel-core/register --reporter mocha-circleci-reporter",
    "e2e-test": "mocha --require babel-core/register ./e2e/**/*.e2e*.js",
    "e2e-test:appveyor1": "mocha --require babel-core/register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.1.js 2> output.err",
    "e2e-test:appveyor2": "mocha --require babel-core/register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.2.js 2> output.err",
    "e2e-test:appveyor3": "mocha --require babel-core/register --reporter mocha-appveyor-reporter ./e2e/**/*.e2e.3.js 2> output.err",
    "e2e-test-circle": "mocha --require babel-core/register --reporter mocha-circleci-reporter ./e2e/**/*.e2e*.js",
    "clean": "rm -rf dist",
    "build": "babel src -d dist",
    "pkg": "pkg . --out-path releases/",
    "watch": "babel --watch bin src -d dist --source-maps",
    "build-debug": "babel src -d dist --source-maps",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "bin": {
    "bit": "bin/bit.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teambit/bit"
  },
  "dependencies": {
    "@teambit/gitconfig": "^2.0.6",
    "ajv": "^6.5.3",
    "array-difference": "^0.0.1",
    "async-exit-hook": "^2.0.1",
    "babel-runtime": "^6.23.0",
    "bit-javascript": "^1.0.6-dev.5",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.3",
    "cli-spinners": "^1.0.0",
    "commander": "^2.12.2",
    "comment-json": "^1.1.3",
    "decamelize": "^1.2.0",
    "deserialize-error": "0.0.3",
    "doctrine": "^2.0.2",
    "execa": "^0.8.0",
    "find-up": "^2.1.0",
    "firstline": "^2.0.2",
    "fs-extra": "^6.0.0",
    "glob": "^7.1.1",
    "graphlib": "^2.1.5",
    "group-array": "^0.3.3",
    "ignore": "^3.3.7",
    "is-array-buffer-x": "^1.0.13",
    "is-glob": "^4.0.0",
    "istextorbinary": "^2.2.1",
    "jfs": "^0.2.6",
    "lodash.assignwith": "^4.2.0",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.0",
    "lodash.omitby": "^4.6.0",
    "lodash.partition": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.toarray": "^4.4.0",
    "lodash.unionby": "^4.8.0",
    "lodash.uniqby": "^4.7.0",
    "loud-rejection": "^1.6.0",
    "mkdirp": "^0.5.1",
    "mockery": "^2.0.0",
    "needle": "^2.0.0",
    "normalize-path": "^2.1.1",
    "object-diff": "^0.0.4",
    "object-hash": "^1.3.0",
    "opn": "^5.3.0",
    "ora": "^1.1.0",
    "p-map-series": "^1.0.0",
    "pad-right": "^0.2.2",
    "parse-gitignore": "^0.4.0",
    "porter-stemmer": "^0.9.1",
    "prompt": "^1.0.0",
    "ramda": "^0.24.1",
    "ramda-adjunct": "^2.9.0",
    "regenerator-runtime": "^0.11.0",
    "requestify": "^0.2.5",
    "roadrunner": "^1.1.0",
    "semver": "^5.4.1",
    "serialize-error": "^2.1.0",
    "ssh2": "^0.5.4",
    "string-format": "^0.5.0",
    "symlink-or-copy": "^1.1.8",
    "table": "^4.0.3",
    "tty-table": "^2.5.5",
    "typescript": "2.9.1",
    "uniqid": "^5.0.3",
    "user-home": "^2.0.0",
    "uuid": "^3.0.1",
    "validate-npm-package-name": "^3.0.0",
    "vinyl": "^2.1.0",
    "vinyl-file": "^3.0.0",
    "winston": "^2.4.0",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-arrays": "^1.0.0",
    "chai-fs": "^1.0.0",
    "chai-string": "^1.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.72.0",
    "flow-coverage-report": "^0.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "mocha": "^5.1.1",
    "mocha-appveyor-reporter": "^0.4.0",
    "mocha-circleci-reporter": "0.0.3",
    "mocha-junit-reporter": "^1.13.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "sinon": "^5.0.7"
  },
  "optionalDependencies": {
    "posix": "^4.1.1"
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
