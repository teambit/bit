{
  "componentId": {
    "name": "ui",
    "version": "0.0.10"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageJson": {
        "name": "@teambit/{name}",
        "private": false
      },
      "packageManagerPublishArgs": ["--access public"]
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "source-map-loader": "1.1.3",
          "babel-loader": "8.2.2",
          "babel-preset-react-app": "10.0.0",
          "file-loader": "6.2.0",
          "resolve-url-loader": "3.1.2",
          "babel-plugin-named-asset-import": "0.3.7",
          "style-loader": "^2.0.0",
          "css-loader": "5.2.0",
          "postcss": "8.2.9",
          "postcss-loader": "5.2.0",
          "postcss-flexbugs-fixes": "5.0.2",
          "sass-loader": "11.0.1",
          "less": "^4.1.1",
          "less-loader": "8.0.0",
          "url-loader": "4.1.1",
          "path-browserify": "1.0.1",
          "@teambit/legacy": "-"
        },
        "devDependencies": {
          "@types/mocha": "5.2.7",
          "@types/node": "12.20.4",
          "@teambit/legacy": "-"
        },
        "peerDependencies": {
          "@teambit/legacy": "1.0.89"
        }
      }
    }
  }
}
