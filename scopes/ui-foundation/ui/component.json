{
  "componentId": {
    "name": "ui",
    "version": "0.0.10"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageJson": {
        "name": "@teambit/{name}",
        "private": false
      },
      "packageManagerPublishArgs": [
        "--access public"
      ]
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "source-map-loader": "1.1.3",
          "babel-loader": "8.2.2",
          "babel-preset-react-app": "9.1.2",
          "style-loader": "1.3.0",
          "file-loader": "6.2.0",
          "resolve-url-loader": "3.1.2",
          "babel-plugin-named-asset-import": "0.3.7",
          "css-loader": "3.6.0",
          "url-loader": "4.1.1",
          "postcss-loader": "3.0.0",
          "sass-loader": "10.1.1",
          "less": "3.13.1",
          "less-loader": "7.3.0",
          "@teambit/legacy": "-"
        },
        "devDependencies": {
          "@types/mocha": "5.2.7",
          "@types/node": "12.20.4",
          "@teambit/legacy": "-"
        },
        "peerDependencies": {
          "@teambit/legacy": "1.0.52"
        }
      }
    }
  }
}
