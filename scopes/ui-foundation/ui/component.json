{
  "componentId": {
    "name": "ui",
    "version": "0.0.10"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageJson": {
        "name": "@teambit/{name}",
        "private": false
      },
      "packageManagerPublishArgs": [
        "--access public"
      ]
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "source-map-loader": "^1.0.0",
          "babel-loader": "^8.1.0",
          "babel-preset-react-app": "^9.1.2",
          "style-loader": "^1.2.1",
          "react-dom": "-",
          "file-loader": "^6.0.0",
          "resolve-url-loader": "^3.1.1",
          "babel-plugin-named-asset-import": "^0.3.6",
          "css-loader": "^3.6.0",
          "url-loader": "^4.1.0",
          "postcss-loader": "^3.0.0",
          "sass-loader": "^8.0.2",
          "less": "^3.12.2",
          "less-loader": "^7.0.2",
          "bit-bin": "-"
        },
        "devDependencies": {
          "@types/mocha": "^5.2.7",
          "@types/node": "^12.12.27",
          "react-dom": "-",
          "bit-bin": "-"
        },
        "peerDependencies": {
          "react": "^16.13.1",
          "react-dom": "^16.13.1",
          "bit-bin": "14.8.9-dev.166"
        }
      }
    }
  }
}
