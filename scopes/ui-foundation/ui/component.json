{
  "componentId": {
    "name": "ui",
    "version": "0.0.10"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageJson": {
        "name": "@teambit/{name}",
        "private": false
      },
      "packageManagerPublishArgs": ["--access public"]
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "source-map-loader": "3.0.0",
          "babel-loader": "8.2.2",
          "babel-preset-react-app": "10.0.0",
          "babel-plugin-named-asset-import": "0.3.7",
          "postcss-loader": "6.1.1",
          "postcss-normalize": "10.0.0",
          "postcss-flexbugs-fixes": "5.0.2",
          "path-browserify": "1.0.1",
          "@teambit/legacy": "-"
        },
        "devDependencies": {
          "@types/mocha": "5.2.7",
          "@types/node": "12.20.4",
          "@types/postcss-normalize": "9.0.1",
          "@teambit/legacy": "-"
        },
        "peerDependencies": {
          "@teambit/legacy": "1.0.153"
        }
      }
    }
  }
}
