{
  "componentId": {
    "name": "react",
    "version": "0.0.10"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageJson": {
        "name": "@teambit/{scope}.{name}",
        "private": false
      },
      "packageManagerPublishArgs": [
        "--access public"
      ]
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "@typescript-eslint/eslint-plugin": "4.13.0",
          "@typescript-eslint/parser": "4.13.0",
          "eslint-config-airbnb-typescript": "12.0.0",
          "@teambit/legacy": "-"
        },
        "devDependencies": {
          "@teambit/legacy": "-"
        },
        "peerDependencies": {
          "eslint": "> 7.0.0",
          "@teambit/legacy": "1.0.89"
        }
      }
    }
  }
}
