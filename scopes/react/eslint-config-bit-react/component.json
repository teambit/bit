{
  "componentId": {
    "name": "react",
    "version": "0.0.10"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageJson": {
        "name": "@teambit/{name}",
        "private": false
      },
      "packageManagerPublishArgs": [
        "--access public"
      ]
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "@typescript-eslint/eslint-plugin": "^4.8.2",
          "@typescript-eslint/parser": "^4.8.2",
          "eslint-config-airbnb-typescript": "12.0.0"
        },
        "peerDependencies": {
          "eslint": "> 7.0.0"
        }
      }
    }
  }
}
